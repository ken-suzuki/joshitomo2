<% if @user.id == current_user.id %>

<% else %>
  <% if @isRoom == true %>
  <%= link_to 'チャットへ', "/rooms/#{@roomId}" %>
  <% else %>
    <%= form_for @room do |f| %>
      <%= fields_for @entry do |e| %>
        <%= e.hidden_field :user_id, :value=> @user.id %>
      <% end %>
      <%= f.submit "チャットを始める" %>
    <% end %>
  <% end %>
<% end %>